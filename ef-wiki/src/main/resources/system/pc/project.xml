        <entity name="ProjectRecommended" label="ProjectRecommended"
                basic="simple" others=""
                model="com.efeiyi.ec.project.model.ProjectRecommended">
            <does>
                <do name="plistProjectRecommended" access="" label="ProjectRecommended列表"
                    result="json">
                    <query name="default" label="默认" type="auto" orderBy="sort:desc">
                        <condition name="status" defaultValue="1" operation="eq"/>
                        <condition name="groupName" defaultValue="ProjectRecommended" operation="eq"/>
                    </query>

                </do>
                <do name="listProjectRecommended" access="" label="ProjectRecommended列表"
                    result="json">
                    <query name="default" label="默认" type="auto" orderBy="sort:desc">
                        <condition name="status" defaultValue="1" operation="eq"/>
                        <condition name="groupName" defaultValue="ProjectRecommended" operation="eq"/>
                    </query>

                </do>
                <do name="listProjectRecommended" label="ProjectRecommended列表" access="" result="/masterProject/masterProjectList">
                    <query name="default1" label="默认" type="auto" orderBy="sort:desc">
                        <condition name="groupName" defaultValue="wiki.projectRecommended" operation="eq"/>
                        <condition name="project.projectCategory.id" defaultValue="1" operation="eq"/>
                        <condition name="status" defaultValue="0" operation="ne"/>
                        <condition name="project.status" defaultValue="0" operation="ne"/>
                    </query>
                </do>
            </does>
