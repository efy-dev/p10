                             alt="">
                    <span class="txt">
                        <c:choose>
                            <c:when test="${project.description.length()<=60}">
                                ${project.description}
                            </c:when>
                            <c:when test="${project.description.length()>60}">
                                ${project.description.substring(0,59)}...
                            </c:when>
                        </c:choose>
                    </span>
