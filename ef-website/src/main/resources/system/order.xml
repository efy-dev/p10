                        <condition name="status" defaultValue="1" operation="eq"/>
                        <condition name="consumer.id" defaultValue="${MY_USER}" operation="eq"/>
                    </query>
                    <query name="byorder" label="可用列表" type="auto" orderBy="id:desc">
                        <condition name="status" defaultValue="1" operation="eq"/>
