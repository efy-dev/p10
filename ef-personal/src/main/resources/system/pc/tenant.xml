        <entity name="masterProject" label="传承人"  model="com.efeiyi.ec.master.model.TenantProject">
                    <does>
                        <do name="listTenantProject" access="" label="传承人列表">
                            <query name="default" label="默认" type="auto" orderBy="id:desc">
                                <condition name="status" defaultValue="0" operation="ne"/>
                            </query>
                        </do>
                    </does>
                </entity>
        <entity name="Tenant" label="传承人列表" model="com.efeiyi.ec.master.model.Master">
            <fields>
                <field name="sex" label="性别" inputType="text">
                    <statusType fieldName="sex">
                        <item label="男" value="1"/>
                        <item label="女" value="2"/>
                    </statusType>
                </field>
                <field name="level" label="等级" inputType="text">
                    <statusType fieldName="level">
                        <item label="国家级" value="1"/>
                        <item label="省级" value="2"/>
                        <item label="市级" value="3"/>
                        <item label="县级" value="4"/>
                    </statusType>
                </field>
            </fields>
            <does>
                <do name="listTenant" access="" label="传承人分页列表">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="" defaultValue="" operation="eq"/>
                    </query>
                </do>
            </does>
        </entity>
        <entity name="tenantInfo" label="传承人资讯"  model="com.efeiyi.ec.master.model.TenantNews">
            <does>
                <do name="plistTenantInfo" access="" label="传承人资讯列表">
                    <query name="default" label="默认" type="auto" orderBy="createDateTime:desc">
                        <condition name="master.id" defaultValue="1" operation="eq"/>
                    </query>
                </do>
            </does>
        </entity>

        <entity name="TenantBanner" label="传承人资讯"  model="com.efeiyi.ec.master.model.TenantBanner">
            <does>
                <do name="listTenantBanner" access="" label="传承人轮播图">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="master.id" defaultValue="1" operation="eq"/>
                    </query>
                </do>
            </does>
        </entity>

        <entity name="TenantIntroduction" label="简介"  model="com.efeiyi.ec.master.model.TenantIntroduction">
            <does>
                <do name="listTenantIntroduction" access="" label="简介列表">
                    <query name="default" label="默认" type="auto">
                        <condition name="master.id" defaultValue="1" operation="eq"/>
                    </query>
                </do>
            </does>
        </entity>
        <entity name="TenantAttachment" label="商品列表"  model="com.efeiyi.ec.master.model.TenantAttachment">
            <does>
                <do name="listAttachment" access="" label="附件列表">
                    <query name="default" label="默认" type="auto" orderBy="">
                        <condition name="introduction.master.id" defaultValue="1" operation="eq"/>
                        <!--<condition name="master.id" defaultValue="1" operation="eq"/>-->
                    </query>
                </do>
            </does>
        </entity>
        <entity name="masterWork" label="传承人作品"  model="com.efeiyi.ec.master.model.TenantWork">
        <does>
            <do name="plistTenantWork" access="" label="传承人作品">
                <query name="default" label="默认" type="auto" orderBy="createDateTime:desc">
                    <condition name="master.id" defaultValue="1" operation="eq"/>
                </query>
            </do>
        </does>
        </entity>
        <entity name="wordValue" label="标签"  model="com.ming800.core.p.model.WordValue">
            <does>
                <do name="listWordValue" access="" label="标签">
                    <query name="default" label="默认" type="auto">
                        <condition name="group" defaultValue="1" operation="eq"/>
                    </query>
                </do>
            </does>
        </entity>
        <entity name="masterProject" label="类别"  model="com.efeiyi.ec.master.model.TenantProject">
            <does>
                <do name="listProject" access="" label="类别">
                    <query name="default" label="默认" type="auto">
                        <condition name="master.id" defaultValue="1" operation="eq"/>
                    </query>
                </do>
            </does>
        </entity>
        <entity name="tenantWorkShop" label="工作坊"  model="com.efeiyi.ec.master.model.TenantWorkShop">
            <does>
                <do name="listTenantWorkShop" access="" label="工作坊">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="master.id" defaultValue="1" operation="eq"/>
                    </query>
                </do>
            </does>
        </entity>
    </entities>
</module>