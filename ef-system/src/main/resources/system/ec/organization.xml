                <do name="listAddressDistrict" label="菜单列表" access="" result="/addressDistrict/addressDistrictList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="菜单主键" name="id" defaultValue="1" operation="eq"/>
