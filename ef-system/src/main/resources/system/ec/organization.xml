<?xml version="1.0" encoding="UTF-8"?>



<module name="organization" label="organization" version="" package="com.efeiyi.ec.organization" path="">

    <entities>
        <entity name="AddressCity" label="市"
                basic="simple" others=""
                model="com.efeiyi.ec.organization.model.AddressCity">
            <does>
                <do name="plistAddressCity" access="" label="市列表"
                    result="/addressCity/addressCityPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="1" operation="eq"/>
                    </query>
                </do>

                <do name="removeAddressCity" label="删除addressCity" access="" result="json"/>
                <do name="formAddressCity" label="保存addressCity" access="" result="/addressCity/addressCityForm"/>
                <do name="saveOrUpdateAddressCity" label="保存addressCity" access="" result="/addressCity/addressCitySaveSuccess"/>
                <do name="listAddressCity" label="addressCity列表" access="" result="/addressCity/addressCityList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="banner主键" name="id" defaultValue="1" operation="eq"/>
                    </query>
                </do>

            </does>
        </entity>
