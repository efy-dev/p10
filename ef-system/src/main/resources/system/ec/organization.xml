                </do>

                <do name="removeBigUser" label="删除省" access="" result="json"/>
                <do name="formBigUser" label="保存省" access="" result="/bigUser/bigUserForm"/>
                <do name="saveOrUpdateBigUser" label="保存省" access="" result="/bigUser/bigUserSaveSuccess"/>
                <do name="listBigUser" label="省列表" access="" result="/bigUser/bigUserList">
