<?xml version="1.0" encoding="UTF-8"?>


<!--用户-->
<module name="tenant" label="系统-传承人" version="" package="com.efeiyi.ec.tenant" path="">

    <entities>
        <entity name="Tenant" label="传承人列表"
                basic="simple" others=""
                model="com.efeiyi.ec.tenant.model.Tenant">
            <fields>
                <field name="id" label="传承人" inputType="text"/>
                <field name="name" label="传承人名字" inputType="text"/>
            </fields>
            <does>
                <do name="plistTenant" access="" label="传承人分页列表"
                    result="/tenant/tenantPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                    </query>
                </do>


                <do name="formTenant" label="保存大师页" access="" result="/tenant/tenantForm"/>
                <do name="viewTenant" label="查看大师页" access="" result="/tenant/tenantView"/>
                <do name="saveOrUpdateTenant" label="保存大师" access="" result="redirect:/basic/xm.do?qm=plistTenant_default" execute="com.efeiyi.ec.system.tenant.controller.TenantHandler">
                    <page>
                        <fields>
                            <field name="createDateTime" label="创建时间" defaultValue="${CURRENT_DATETIME}" inputType="default"></field>
                        </fields>
                    </page>
                </do>
                <do name="removeTenant" label="删除大师" access="" result="json"/>
                <do name="listTenant" label="传承人列表" access="" result="/tenant/tenantList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="主键" name="id" defaultValue="0" peration="ne"/>
                    </query>
                </do>

            </does>
        </entity>
        <entity name="TenantBanner" label="传承人轮播图列表"
                basic="simple" others=""
                model="com.efeiyi.ec.tenant.model.TenantBanner">
            <does>
                <do name="plistTenantBanner" access="" label="传承人轮播图分页列表"
                    result="/tenantBanner/tenantBannerPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                    </query>
                </do>


                <do name="formTenantBanner" label="保存传承人轮播图" access="" result="/tenantBanner/tenantBannerForm" execute="com.efeiyi.ec.system.tenant.controller.ToTenantBannerFormHandler"/>
                <do name="viewTenantBanner" label="查看传承人轮播图" access="" result="/tenantBanner/tenantBannerView"/>
                <do name="saveOrUpdateTenantBanner" label="保存传承人轮播图" access="" execute="com.efeiyi.ec.system.tenant.controller.TenantBannerHandler">
                </do>
                <do name="removeTenantBanner" label="删除传承人轮播图" access="" result="json"/>
                <do name="listTenantBanner" label="传承人轮播图列表" access="" result="/tenantBanner/tenantBannerList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">

                        <condition label="状态" name="status" defaultValue="1" operation="eq"/>
                    </query>
                </do>

            </does>
        </entity>
        <entity name="Business" label="商家列表"
                basic="simple" others=""
                model="com.efeiyi.ec.tenant.model.Tenant">
            <does>
                <do name="plistBusiness" access="" label="传承人分页列表"
                    result="/tenant/tenantPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                    </query>
                </do>

                <do name="formBusiness" label="保存大师页" access="" result="/tenant/tenantForm"/>
                <do name="saveOrUpdateBusiness" label="保存大师" access="" result="/tenant/tenantSaveSuccess">
                </do>
                <do name="removeBusiness" label="删除大师" access="" result="json"/>
                <do name="listBusiness" label="传承人列表" access="" result="/tenant/tenantList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <!--<condition label="产品主键" name="id" defaultValue="1"o peration="eq"/>-->
                        <condition label="产品名称" name="name" defaultValue="1" operation="eq"/>
                        <!--<condition label="产品序列" name="serial" defaultValue="1" operation="eq"/>-->
                    </query>
                </do>

            </does>
        </entity>
        <entity name="TenantWorkShop" label="传承人工作坊列表"
                basic="simple" others=""
                model="com.efeiyi.ec.tenant.model.TenantWorkShop">
            <fields>
                <field name="id" label="传承人工作坊id" inputType="text"/>
                <field name="name" label="传承人工作坊名字" inputType="text"/>
            </fields>
            <does>
                <do name="plistTenantWorkShop" access="" label="传承人工作坊分页列表"
                    result="/tenantWorkShop/tenantWorkShopPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="1" operation="eq"/>
                    </query>
                </do>

                <do name="formTenantWorkShop" label="保存工作坊页" access="" result="/tenantWorkShop/tenantWorkShopForm"/>
                <do name="saveOrUpdateTenantWorkShop" label="保存工作坊著作" access="" result="/tenantWorkShop/tenantWorkShopSaveSuccess">
                </do>
                <do name="removeTenantWorkShop" label="删除工作坊著作" access="" result="json"/>
                <do name="listTenantWorkShop" label="大师工作坊列表" access="" result="/tenantWorkShop/tenantWorkShopList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="工作坊id" name="id" defaultValue="1" operation="eq"/>
                    </query>
                </do>

            </does>
        </entity>
        <entity name="TenantHonor" label="传承人殊荣列表"
                basic="simple" others=""
                model="com.efeiyi.ec.tenant.model.TenantHonor">
            <fields>
                <field name="id" label="传承人殊荣id" inputType="text"/>
                <field name="memo" label="传承人殊荣memo" inputType="text"/>
            </fields>
            <does>
                <do name="plistTenantHonor" access="" label="传承人殊荣分页列表"
                    result="/tenantHonor/tenantHonorPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="1" operation="eq"/>
                    </query>
                </do>

                <do name="formTenantHonor" label="保存大师殊荣页" access="" result="/tenantHonor/tenantHonorForm"/>
                <do name="saveOrUpdateTenantHonor" label="保存大师殊荣" access="" result="/tenantHonor/tenantHonorSaveSuccess">
                </do>
                <do name="removeTenantHonor" label="删除大师殊荣" access="" result="json"/>
                <do name="listTenantHonor" label="传承人列表" access="" result="/tenantHonor/tenantHonorList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="大师殊荣id" name="id" defaultValue="1" operation="eq"/>
                    </query>
                </do>

            </does>
        </entity>
        <entity name="TenantIntroduction" label="传承人简介列表"
                basic="simple" others=""
                model="com.efeiyi.ec.tenant.model.TenantIntroduction">
            <fields>
                <field name="id" label="传承人简介id" inputType="text"/>
            </fields>
            <does>
                <do name="plistTenantIntroduction" access="" label="传承人简介分页列表"
                    result="/tenantIntroduction/tenantIntroductionPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                    </query>


                </do>
                <do name="formTenantIntroduction" label="保存大师简介页" access="" result="/tenantIntroduction/tenantIntroductionForm" execute="com.efeiyi.ec.system.tenant.controller.TenantIntroductionFormHandler"/>
                <do name="viewTenantIntroduction" label="查看大师简介页" access="" result="/tenantIntroduction/tenantIntroductionView"/>
                <do name="saveOrUpdateTenantIntroduction" label="保存大师简介" access="" result="redirect:/basic/xm.do?qm=plistTenantIntroduction_default">
                    <page>
                        <fields>
                            <field name="status" label="状态" defaultValue="1"
                                   inputType="default"></field>
                        </fields>
                    </page>
                </do>
                <do name="removeTenantIntroduction" label="删除大师简介" access="" result="json"/>
                <do name="listTenantIntroduction" label="传承人简介列表" access="" result="/tenantIntroduction/tenantIntroductionIncludeList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <!--<condition label="产品主键" name="id" defaultValue="1"o peration="eq"/>-->
                        <!--<condition label="传承人简介id" name="id" defaultValue="0" operation="ne"/>-->
                        <condition label="传承人id" name="tenant.id" operation="eq"/>
                        <!--<condition label="产品序列" name="serial" defaultValue="1" operation="eq"/>-->
                    </query>


                    <query name="include" label="嵌入" type="auto" orderBy="id:desc">
                        <!--<condition name="id" defaultValue="0" operation="ne"/>-->
                        <condition label="传承人id" name="tenant.id" operation="eq"/>
                        <condition label="状态" name="status" defaultValue="1" operation="eq"/>
                    </query>
                </do>

            </does>
        </entity>

        <entity name="TenantAttachment" label="传承人介绍附件"
                basic="simple" others=""
                model="com.efeiyi.ec.tenant.model.TenantAttachment">
            <does>
                <do name="saveOrUpdateTenantAttachment" label="保存附件" access="" execute="com.efeiyi.ec.system.tenant.controller.TenantAttachmentHandler">
                    <page>
                        <fields>
                            <field name="status" label="状态" defaultValue="1"
                                   inputType="default"></field>
                        </fields>
                    </page>
                </do>
                <do name="removeTenantAttachment" label="删除附件" access="" result="json"/>
            </does>
        </entity>


        <entity name="TenantNews" label="传承人新闻列表"
                basic="simple" others=""
                model="com.efeiyi.ec.tenant.model.TenantNews">
            <does>
                <do name="plistTenantNews" access="" label="传承人新闻分页列表"
                    result="/tenantNews/tenantNewsPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                    </query>
                </do>

                <do name="formTenantNews" label="保存大师新闻页" access="" result="/tenantNews/tenantNewsForm"/>
                <do name="saveOrUpdateTenantNews" label="保存大师新闻" access="" result="/tenantNews/tenantNewsSaveSuccess">
                </do>
                <do name="removeTenantNews" label="删除大师新闻" access="" result="json"/>
                <do name="listTenantNews" label="传承人新闻列表" access="" result="/tenantNews/tenantNewsList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <!--<condition label="产品主键" name="id" defaultValue="1"o peration="eq"/>-->
                        <condition label="传承人新闻id" name="id" defaultValue="0" operation="ne"/>
                        <!--<condition label="产品序列" name="serial" defaultValue="1" operation="eq"/>-->
                    </query>
                </do>

            </does>
        </entity>
        <entity name="TenantWork" label="传承人产品列表"
                basic="simple" others=""
                model="com.efeiyi.ec.tenant.model.TenantWork">
            <fields>
                <field name="id" label="传承人产品id" inputType="text"/>
                <field name="name" label="传承人产品名字" inputType="text"/>
            </fields>
            <does>
                <do name="plistTenantWork" access="" label="传承人产品分页列表"
                    result="/tenantWork/tenantWorkPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                    </query>
                </do>

                <do name="formTenantWork" label="保存传承人产品页" access="" result="/tenantWork/tenantWorkForm" execute="com.efeiyi.ec.system.tenant.controller.TenantWorkHandler"/>
                <do name="viewTenantWork" label="查看传承人产品页" access="" result="/tenantWork/tenantWorkView"/>
                <do name="saveOrUpdateTenantWork" label="保存传承人产品" access="" result="/tenantWork/tenantWorkSaveSuccess">
                </do>
                <do name="removeTenantWork" label="删除大师产品" access="" result="json"/>
                <do name="listTenantWork" label="大师著作产品" access="" result="/tenantWork/tenantWorkIncludeList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="产品id" name="id" defaultValue="0" operation="ne"/>
                    </query>

                    <query name="include" label="嵌入" type="auto" orderBy="id:desc">
                        <condition label="状态" name="status" defaultValue="1" operation="eq"/>
                        <condition label="传承人Id" name="tenant.id" operation="eq"/>

                    </query>
                </do>

            </does>
        </entity>
    </entities>
</module>