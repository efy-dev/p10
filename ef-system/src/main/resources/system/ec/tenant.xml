                    </query>


                </do>
                <do name="formTenantIntroduction" label="保存大师简介页" access="" result="/masterIntroduction/tenantIntroductionForm" execute="com.efeiyi.ec.system.master.controller.TenantIntroductionFormHandler"/>
                <do name="viewTenantIntroduction" label="查看大师简介页" access="" result="/masterIntroduction/tenantIntroductionView"/>
                <do name="saveOrUpdateTenantIntroduction" label="保存大师简介" access="" result="redirect:/basic/xm.do?qm=plistTenantIntroduction_default">
                    <page>
                        <fields>
                            <field name="status" label="状态" defaultValue="1"
