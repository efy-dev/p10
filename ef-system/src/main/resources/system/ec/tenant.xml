<?xml version="1.0" encoding="UTF-8"?>


<!--用户-->
<module name="master" label="系统-传承人" version="" package="com.efeiyi.ec.master" path="">

    <entities>
        <entity name="Tenant" label="传承人列表"
                basic="simple" others=""
                model="com.efeiyi.ec.master.model.Master">
            <fields>
                <field name="id" label="传承人" inputType="text"/>
                <field name="name" label="传承人名字" inputType="text"/>
                <field name="sex" label="性别" inputType="text">
                    <statusType fieldName="sex">
                        <item label="男" value="1"/>
                        <item label="女" value="2"/>
                    </statusType>
                </field>
                <field name="level" label="等级" inputType="text">
                    <statusType fieldName="level">
                        <item label="国家级" value="1"/>
                        <item label="省级" value="2"/>
                        <item label="市级" value="3"/>
                        <item label="县级" value="4"/>
                    </statusType>
                </field>
            </fields>
            <does>
                <do name="plistTenant" access="" label="传承人分页列表"
                    result="/master/tenantPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                        <condition name="status" defaultValue="1" operation="eq"/>
                    </query>
                </do>


                <do name="formTenant" label="保存大师页" access="" result="/master/tenantForm"/>
                <do name="viewTenant" label="查看大师页" access="" result="/master/tenantView"/>
                <do name="saveOrUpdateTenant" label="保存大师" access="" result="redirect:/basic/xm.do?qm=plistTenant_default" execute="com.efeiyi.ec.system.master.controller.TenantHandler">
                    <page>
                        <fields>
                            <field name="createDateTime" label="创建时间" defaultValue="${CURRENT_DATETIME}" inputType="default"></field>
                            <field name="stauts" label="创建时间" defaultValue="1" inputType="default"></field>
                        </fields>
                    </page>
                </do>
                <do name="removeTenant" label="删除大师" access="" result="json"/>
                <do name="listTenant" label="传承人列表" access="" result="/master/masterList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="主键" name="id" defaultValue="0" peration="ne"/>
                    </query>
                </do>

            </does>
        </entity>
        <entity name="TenantBanner" label="传承人轮播图列表"
                basic="simple" others=""
                model="com.efeiyi.ec.master.model.TenantBanner">
            <does>
                <do name="plistTenantBanner" access="" label="传承人轮播图分页列表"
                    result="/tenantBanner/tenantBannerPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                    </query>
                </do>


                <do name="formTenantBanner" label="保存传承人轮播图" access="" result="/tenantBanner/tenantBannerForm" execute="com.efeiyi.ec.system.master.controller.ToTenantBannerFormHandler"/>
                <do name="viewTenantBanner" label="查看传承人轮播图" access="" result="/tenantBanner/tenantBannerView"/>
                <do name="saveOrUpdateTenantBanner" label="保存传承人轮播图" access="" execute="com.efeiyi.ec.system.master.controller.TenantBannerHandler">
                </do>
                <do name="removeTenantBanner" label="删除传承人轮播图" access="" result="json"/>
                <do name="listTenantBanner" label="传承人轮播图列表" access="" result="/tenantBanner/tenantBannerList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="状态" name="status" defaultValue="1" operation="eq"/>
                        <condition label="状态" name="master.id"  operation="eq"/>
                    </query>
                </do>

            </does>
        </entity>
        <entity name="Business" label="商家列表"
                basic="simple" others=""
                model="com.efeiyi.ec.master.model.Master">
            <does>
                <do name="plistBusiness" access="" label="传承人分页列表"
                    result="/master/tenantPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                    </query>
                </do>

                <do name="formBusiness" label="保存大师页" access="" result="/master/tenantForm"/>
                <do name="saveOrUpdateBusiness" label="保存大师" access="" result="/master/tenantSaveSuccess">
                </do>
                <do name="removeBusiness" label="删除大师" access="" result="json"/>
                <do name="listBusiness" label="传承人列表" access="" result="/master/masterList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <!--<condition label="产品主键" name="id" defaultValue="1"o peration="eq"/>-->
                        <condition label="产品名称" name="name" defaultValue="1" operation="eq"/>
                        <!--<condition label="产品序列" name="serial" defaultValue="1" operation="eq"/>-->
                    </query>
                </do>

            </does>
        </entity>
        <entity name="TenantWorkShop" label="传承人工作坊列表"
                basic="simple" others=""
                model="com.efeiyi.ec.master.model.TenantWorkShop">
