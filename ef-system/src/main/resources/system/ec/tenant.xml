        <entity name="TenantWork" label="传承人产品列表"
                basic="simple" others=""
                model="com.efeiyi.ec.master.model.TenantWork">
            <fields>
                <field name="id" label="传承人产品id" inputType="text"/>
                <field name="name" label="传承人产品名字" inputType="text"/>
            </fields>
            <does>
                <do name="plistTenantWork" access="" label="传承人产品分页列表"
                    result="/masterWork/tenantWorkPList" execute="com.efeiyi.ec.system.master.controller.TenantWorkPListHandler">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                    </query>

                    <query name="byProject" label="默认" type="auto" orderBy="id:desc">
                        <condition name="project.id" operation="eq"/>
                        <condition name="status" defaultValue="1" operation="eq"/>
                    </query>
                </do>


                <do name="viewTenantWorkImg" label="查看传承人产品页" access="" result="/masterWork/imgView"/>
                <do name="formTenantWork" label="保存传承人产品页" access="" result="/masterWork/tenantWorkForm" execute="com.efeiyi.ec.system.master.controller.TenantWorkHandler"/>
                <do name="viewTenantWork" label="查看传承人产品页" access="" result="/masterWork/tenantWorkView"/>
                <do name="saveOrUpdateTenantWork" label="保存传承人产品" access="" result="redirect:/basic/xm.do?qm=plistTenantWork_byProject" execute="com.efeiyi.ec.system.master.controller.TenantWorkFormHandler">
                    <page>
                        <fields>
                            <field name="status" label="状态" defaultValue="1"
                                   inputType="default"></field>
                        </fields>
                    </page>
                </do>
                <do name="removeTenantWork" label="删除大师产品" access="" result="json"/>
                <do name="listTenantWork" label="大师著作产品" access="" result="/masterWork/tenantWorkIncludeList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="状态" name="status" defaultValue="1" operation="eq"/>
                    </query>

                    <query name="include" label="嵌入" type="auto" orderBy="id:desc">
                        <condition label="状态" name="status" defaultValue="1" operation="eq"/>
                        <condition label="传承人Id" name="master.id" operation="eq"/>
                    </query>
                </do>

            </does>
        </entity>

