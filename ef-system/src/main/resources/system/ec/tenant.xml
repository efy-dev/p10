                <do name="formTenantWorkShop" label="保存工作坊页" access="" result="/tenantWorkShop/tenantWorkShopForm" execute="com.efeiyi.ec.system.master.controller.TenantWorkShopFormHandler"/>
                <do name="viewTenantWorkShop" label="查看工作坊" access="" result="/tenantWorkShop/tenantWorkShopIncludeView"/>
                <do name="saveOrUpdateTenantWorkShop" label="保存工作坊著作" access="" result="/tenantWorkShop/tenantWorkShopSaveSuccess" execute="com.efeiyi.ec.system.master.controller.TenantWorkShopHandler">
                    <page>
                        <fields>
                            <field name="status" label="状态" defaultValue="1"
                                   inputType="default"/>
                            <field name="createDateTime" label="状态" defaultValue="${CURRENT_DATETIME}"
                                   inputType="default"/>
                        </fields>
                    </page>
                </do>
                <do name="removeTenantWorkShop" label="删除工作坊著作" access="" result="json"/>
                <do name="listTenantWorkShop" label="大师工作坊列表" access="" result="/tenantWorkShop/tenantWorkShopIncludeList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="状态" name="status" defaultValue="1" operation="eq"/>
                        <condition label="传承人" name="master.id" operation="eq"/>
                    </query>
                </do>

            </does>
        </entity>
        <entity name="TenantHonor" label="传承人殊荣列表"
