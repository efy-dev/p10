<?xml version="1.0" encoding="UTF-8"?>


<!--用户-->
<module name="tenantWork" label="系统-传承人著作" version="" package="com.efeiyi.ec.tenant" path="">

    <entities>
        <entity name="TenantWorkList" label="传承人著作列表"
                basic="simple" others=""
                model="com.efeiyi.ec.tenant.model.TenantWork">
            <fields>
                <field name="id" label="传承人著作id" inputType="text"/>
                <field name="name" label="传承人著作名字" inputType="text"/>
            </fields>
            <does>
                <do name="plistTenantWork" access="" label="传承人著作分页列表"
                    result="/tenantWork/tenantWorkPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="1" operation="eq"></condition>
                    </query>
                </do>

                <do name="formTenantWork" label="保存大师著作页" access="" result="/tenantWork/tenantWorkForm"></do>
                <do name="saveOrUpdateTenantWork" label="保存大师著作" access="" result="/tenantWork/tenantWorkSaveSuccess">
                </do>
                <do name="removeTenantWork" label="删除大师著作" access="" result="json"/>
                <do name="listTenantWork" label="大师著作列表" access="" result="/tenantWork/tenantWorkList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="著作id" name="id" defaultValue="1" operation="eq"/>
                    </query>
                </do>

            </does>
        </entity>

    </entities>

</module>