                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="状态" name="status" defaultValue="1" operation="eq"/>
                        <condition label="传承人" name="master.id" operation="eq"/>
                    </query>
                </do>

            </does>
        </entity>

        <entity name="MasterIntroduction" label="传承人简介列表"
                basic="simple" others=""
                model="com.efeiyi.ec.master.model.MasterIntroduction">
            <fields>
                <field name="id" label="传承人简介id" inputType="text"/>
                <field name="type" label="简介类型" inputType="text">
                    <statusType fieldName="type">
                        <item label="基本信息" value="1"/>
                        <item label="大师荣誉" value="2"/>
                        <item label="出版著作" value="3"/>
                        <item label="艺术年表" value="4"/>
                        <item label="工作坊" value="5"/>
                    </statusType>
                </field>
            </fields>
            <does>
                <do name="plistMasterIntroduction" access="" label="传承人简介分页列表"
                    result="/masterIntroduction/masterIntroductionPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                    </query>


                </do>
                <do name="formMasterIntroduction" label="保存大师简介页" access=""
                    result="/masterIntroduction/masterIntroductionForm"
                    execute="com.efeiyi.ec.system.master.controller.MasterIntroductionFormHandler"/>
                <do name="viewMasterIntroduction" label="查看大师简介页" access=""
                    result="/masterIntroduction/masterIntroductionView"/>
                <do name="saveOrUpdateMasterIntroduction" label="保存大师简介" access=""
                    result="redirect:/basic/xm.do?qm=plistMasterIntroduction_default">
                    <page>
                        <fields>
                            <field name="status" label="状态" defaultValue="1"
                                   inputType="default"/>
                        </fields>
                    </page>
                </do>
                <do name="removeMasterIntroduction" label="删除大师简介" access="" result="json"/>
                <do name="listMasterIntroduction" label="传承人简介列表" access=""
                    result="/masterIntroduction/masterIntroductionIncludeList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <!--<condition label="产品主键" name="id" defaultValue="1"o peration="eq"/>-->
                        <!--<condition label="传承人简介id" name="id" defaultValue="0" operation="ne"/>-->
                        <condition label="传承人id" name="master.id" operation="eq"/>
                        <!--<condition label="产品序列" name="serial" defaultValue="1" operation="eq"/>-->
                    </query>


                    <query name="include" label="嵌入" type="auto" orderBy="id:desc">
                        <!--<condition name="id" defaultValue="0" operation="ne"/>-->
                        <condition label="传承人id" name="master.id" operation="eq"/>
                        <condition label="状态" name="status" defaultValue="1" operation="eq"/>
                    </query>
                </do>

            </does>
        </entity>

        <entity name="MasterIntroductionAttachment" label="传承人介绍附件"
                basic="simple" others=""
                model="com.efeiyi.ec.master.model.MasterIntroductionAttachment">
            <does>
                <do name="saveOrUpdateMasterAttachment" label="保存附件" access=""
                    execute="com.efeiyi.ec.system.master.controller.MasterAttachmentHandler">
                    <page>
                        <fields>
                            <field name="status" label="状态" defaultValue="1"
                                   inputType="default"/>
                        </fields>
                    </page>
                </do>

                <do name="removeMasterAttachment" label="删除附件" access="" result="json"/>

                <do name="listMasterAttachment" label="传承人附件列表" access=""
                    result="/masterIntroduction/masterAttachmentIncludeList">
                    <query name="include" label="嵌入" type="auto" orderBy="id:desc">
                        <condition label="简介Id" name="introduction.id" operation="eq"/>
                        <condition label="状态" name="status" defaultValue="1" operation="eq"/>
                    </query>
                </do>

            </does>
        </entity>


