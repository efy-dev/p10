                <do name="formMasterWorkShop" label="保存工作坊页" access="" result="/masterWorkShop/masterWorkShopForm" execute="com.efeiyi.ec.system.master.controller.MasterWorkShopFormHandler"/>
                <do name="viewMasterWorkShop" label="查看工作坊" access="" result="/masterWorkShop/masterWorkShopIncludeView"/>
                <do name="saveOrUpdateMasterWorkShop" label="保存工作坊著作" access="" result="/masterWorkShop/masterWorkShopSaveSuccess" execute="com.efeiyi.ec.system.master.controller.MasterWorkShopHandler">
                    <page>
                        <fields>
                            <field name="status" label="状态" defaultValue="1"
                                   inputType="default"/>
                            <field name="createDateTime" label="状态" defaultValue="${CURRENT_DATETIME}"
                                   inputType="default"/>
                        </fields>
                    </page>
                </do>
                <do name="removeMasterWorkShop" label="删除工作坊著作" access="" result="json"/>
                <do name="listMasterWorkShop" label="大师工作坊列表" access="" result="/masterWorkShop/masterWorkShopIncludeList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="状态" name="status" defaultValue="1" operation="eq"/>
                        <condition label="传承人" name="master.id" operation="eq"/>
                    </query>
                </do>

            </does>
        </entity>
        <entity name="MasterHonor" label="传承人殊荣列表"
