<?xml version="1.0" encoding="UTF-8"?>


<!--用户-->
<module name="master" label="系统-传承人" version="" package="com.efeiyi.ec.master" path="">

    <entities>
        <entity name="Master" label="传承人列表"
                basic="simple" others=""
                model="com.efeiyi.ec.master.model.Master">
            <fields>
                <field name="id" label="传承人" inputType="text"/>
                <field name="name" label="传承人名字" inputType="text"/>
                <field name="sex" label="性别" inputType="text">
                    <statusType fieldName="sex">
                        <item label="男" value="1"/>
                        <item label="女" value="2"/>
                    </statusType>
                </field>
                <field name="level" label="等级" inputType="text">
                    <statusType fieldName="level">
                        <item label="国家级" value="1"/>
                        <item label="省级" value="2"/>
                        <item label="市级" value="3"/>
                        <item label="县级" value="4"/>
                    </statusType>
                </field>
            </fields>
            <does>
                <do name="plistMaster" access="" label="传承人分页列表"
                    result="/master/masterPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                        <condition name="status" defaultValue="1" operation="eq"/>
                    </query>
                </do>


                <do name="formMaster" label="保存大师页" access="" result="/master/masterForm"/>
                <do name="viewMaster" label="查看大师页" access="" result="/master/masterView"/>
                <do name="saveOrUpdateMaster" label="保存大师" access="" result="redirect:/basic/xm.do?qm=plistMaster_default" execute="com.efeiyi.ec.system.master.controller.MasterHandler">
                    <page>
                        <fields>
                            <field name="createDateTime" label="创建时间" defaultValue="${CURRENT_DATETIME}" inputType="default"></field>
                            <field name="status" label="创建时间" defaultValue="1" inputType="default" />
                        </fields>
                    </page>
                </do>
                <do name="removeMaster" label="删除大师" access="" result="json"/>
                <do name="listMaster" label="传承人列表" access="" result="/master/masterList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="主键" name="id" defaultValue="0" peration="ne"/>
                    </query>
                </do>

            </does>
        </entity>
        <entity name="MasterBanner" label="传承人轮播图列表"
                basic="simple" others=""
                model="com.efeiyi.ec.master.model.MasterBanner">
            <does>
                <do name="plistMasterBanner" access="" label="传承人轮播图分页列表"
                    result="/masterBanner/masterBannerPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                    </query>
                </do>


                <do name="formMasterBanner" label="保存传承人轮播图" access="" result="/masterBanner/masterBannerForm" execute="com.efeiyi.ec.system.master.controller.ToMasterBannerFormHandler"/>
                <do name="viewMasterBanner" label="查看传承人轮播图" access="" result="/masterBanner/masterBannerView"/>
                <do name="saveOrUpdateMasterBanner" label="保存传承人轮播图" access="" execute="com.efeiyi.ec.system.master.controller.MasterBannerHandler">
                </do>
                <do name="removeMasterBanner" label="删除传承人轮播图" access="" result="json"/>
                <do name="listMasterBanner" label="传承人轮播图列表" access="" result="/masterBanner/masterBannerList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="状态" name="status" defaultValue="1" operation="eq"/>
                        <condition label="状态" name="master.id"  operation="eq"/>
                    </query>
                </do>

            </does>
        </entity>
        <entity name="Business" label="商家列表"
                basic="simple" others=""
                model="com.efeiyi.ec.master.model.Master">
            <does>
                <do name="plistBusiness" access="" label="传承人分页列表"
                    result="/master/masterPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                    </query>
                </do>

                <do name="formBusiness" label="保存大师页" access="" result="/master/masterForm"/>
                <do name="saveOrUpdateBusiness" label="保存大师" access="" result="/master/masterSaveSuccess">
                </do>
                <do name="removeBusiness" label="删除大师" access="" result="json"/>
                <do name="listBusiness" label="传承人列表" access="" result="/master/masterList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <!--<condition label="产品主键" name="id" defaultValue="1"o peration="eq"/>-->
                        <condition label="产品名称" name="name" defaultValue="1" operation="eq"/>
                        <!--<condition label="产品序列" name="serial" defaultValue="1" operation="eq"/>-->
                    </query>
                </do>

            </does>
        </entity>
        <entity name="MasterWorkShop" label="传承人工作坊列表"
                basic="simple" others=""
                model="com.efeiyi.ec.master.model.MasterWorkShop">
            <fields>
                <field name="id" label="传承人工作坊id" inputType="text"/>
