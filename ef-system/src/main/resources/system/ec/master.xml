                    execute="com.efeiyi.ec.system.master.controller.MasterWorkShopHandler">
                    <page>
                        <fields>
                            <field name="status" label="状态" defaultValue="1"
                                   inputType="default"/>
                            <field name="createDateTime" label="状态" defaultValue="${CURRENT_DATETIME}"
                                   inputType="default"/>
