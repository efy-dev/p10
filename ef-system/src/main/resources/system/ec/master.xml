        <entity name="MasterWork" label="传承人产品列表"
                basic="simple" others=""
                model="com.efeiyi.ec.master.model.MasterWork">
            <fields>
                <field name="id" label="传承人产品id" inputType="text"/>
                <field name="name" label="传承人产品名字" inputType="text"/>
            </fields>
            <does>
                <do name="plistMasterWork" access="" label="传承人产品分页列表"
                    result="/masterWork/masterWorkPList"
                    execute="com.efeiyi.ec.system.master.controller.MasterWorkPListHandler">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="master.id" defaultValue="1" operation="eq"/>
                    </query>
                    <query name="byProject" label="默认" type="auto" orderBy="id:desc">
                        <condition name="project.id" operation="eq"/>
                        <condition name="master.id" operation="eq"/>
                        <condition name="status" defaultValue="1" operation="eq"/>
                    </query>
                    <query name="getProduct2" label="默认" type="auto" orderBy="id:desc">
                        <condition name="project.id" operation="eq"/>
                        <condition name="status" defaultValue="1" operation="eq"/>
                    </query>
                    <query name="byList" label="默认" type="auto" orderBy="createDateTime:desc">
                        <condition name="status" defaultValue="1" operation="eq"/>
                    </query>
                    <query name="default1" label="快速查询" type="auto" orderBy="createDateTime:desc">
                        <condition name="status" defaultValue="0" operation="ne" label="状态" inputType="default"/>
                        <condition name="name" defaultValue="" operation="like" label="作品名称" inputType="text"/>
                        <condition name="master.name" defaultValue="" operation="like" label="大师名称" inputType="text"/>
                    </query>
                </do>
                <do name="plistMasterWork2" access="" label="工艺大师"
                    result="/masterWork/masterWorkPList2">
                    <query name="default" label="默认" type="auto" orderBy="createDateTime:desc">
                        <condition name="status" defaultValue="1" operation="eq"/>
                    </query>
                    <query name="default1" label="快速查询" type="auto" orderBy="createDateTime:desc">
                        <condition name="status" defaultValue="0" operation="ne" label="状态" inputType="default"/>
                        <condition name="name" defaultValue="" operation="like" label="作品名称" inputType="text"/>
                        <condition name="master.fullName" defaultValue="" operation="like" label="作者" inputType="text"/>
                        <condition name="category.name" defaultValue="" operation="like" label="类别" inputType="text"/>
