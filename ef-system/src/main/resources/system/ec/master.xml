            </fields>
            <does>
                <do name="plistMasterWork" access="" label="传承人产品分页列表"
                    result="/masterWork/masterWorkPList" execute="com.efeiyi.ec.system.master.controller.MasterWorkPListHandler">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
