                <do name="viewMasterIntroduction" label="查看大师简介页" access="" result="/masterIntroduction/masterIntroductionView"/>
                <do name="saveOrUpdateMasterIntroduction" label="保存大师简介" access="" result="redirect:/basic/xm.do?qm=plistMasterIntroduction_default">
                    <page>
                        <fields>
                            <field name="status" label="状态" defaultValue="1"
