<?xml version="1.0" encoding="UTF-8"?>


<module name="master" label="系统-传承人" version="" package="com.efeiyi.ec.master" path="">

    <entities>
        <entity name="Master" label="传承人列表"
                basic="simple" others=""
                model="com.efeiyi.ec.master.model.Master">
            <fields>
                <field name="id" label="传承人" inputType="text"/>
                <field name="name" label="传承人名字" inputType="text"/>
                <field name="sex" label="性别" inputType="text">
                    <statusType fieldName="sex">
                        <item label="男" value="1"/>
                        <item label="女" value="2"/>
                    </statusType>
                </field>
                <field name="level" label="等级" inputType="text">
                    <statusType fieldName="level">
                        <item label="国家级" value="1"/>
                        <item label="省级" value="2"/>
                        <item label="市级" value="3"/>
                        <item label="县级" value="4"/>
                    </statusType>
                </field>
            </fields>
            <does>
                <do name="plistMaster" access="" label="传承人列表"
                    result="/master/masterPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                        <condition name="status" defaultValue="1" operation="eq"/>
                    </query>
                    <query name="default1" label="快速查询" type="auto" orderBy="id:desc">
                        <condition name="fullName" operation="like" label="大师名称" inputType="text"/>
                        <condition name="level" operation="eq" label="大师级别" inputType="select_status"
                                   key="Master.level"/>
                        <condition name="status" defaultValue="0" operation="ne" inputType="default"/>
                    </query>
                </do>
                <do name="plistECMaster" access="" label="传承人分页列表"
                    result="/master/masterECPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                        <condition name="status" defaultValue="1" operation="eq"/>
                    </query>
                </do>
                <do name="plistTMaster" access="" label="传承人分页列表"
                    execute="com.efeiyi.ec.system.master.controller.TenantMasterListHandler"
                    result="/tenantMaster/tenantMasterPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne" inputType="default"/>
                        <condition name="status" defaultValue="1" operation="eq" inputType="default"/>
                    </query>
                    <query name="default1" label="查询" type="auto" orderBy="id:desc">
                        <condition name="fullName"  operation="eq" label="大师名称" inputType="text"/>
                        <condition name="status" defaultValue="1" operation="eq" inputType="default"/>
                    </query>
                </do>
                <do name="plistEcMaster" access="" label="传承人分页列表"
                    result="/ecMaster/ecMasterPList">
                    <query name="index" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne" inputType="default"/>
                        <condition name="status" defaultValue="1" operation="eq" inputType="default"/>
                    </query>
                    <query name="search" label="查询" type="auto" orderBy="id:desc">
                        <condition name="sex" operation="eq" inputType="select_status" label="性别" key="Master.sex"/>
                        <condition name="level" operation="eq" inputType="select_status" label="等级" key="Master.level"/>
                        <condition name="fullName" operation="likes" inputType="text" label="传承人名字"/>
                    </query>
                </do>
                <do name="formMaster" label="保存大师页" access="" result="/master/masterForm" execute="com.efeiyi.ec.system.master.controller.MasterFormHandler"/>
                <do name="viewMaster" label="查看大师页" access="" result="/master/masterView"/>
                <do name="viewEcMaster" label="查看大师页" access="" result="/ecMaster/ecMasterView"/>
                <do name="viewMasterwiki" label="查看大师页" access="" result="/master/masterViewwiki"/>
                <do name="saveOrUpdateMaster" label="保存大师" access=""
                    result="redirect:/basic/xm.do?qm=plistMaster_default"
                    execute="com.efeiyi.ec.system.master.controller.MasterHandler">
                    <page>
                        <fields>
                            <field name="createDateTime" label="创建时间" defaultValue="${CURRENT_DATETIME}"
                                   inputType="default"/>
                            <field name="status" label="创建时间" defaultValue="1" inputType="default"/>
                        </fields>
                    </page>
                </do>
                <do name="saveOrUpdateEcMaster" label="保存大师" access=""
                    result="redirect:/basic/xm.do?qm=plistMaster_default&amp;param=EcMaster"
                    execute="com.efeiyi.ec.system.master.controller.MasterHandler">
                    <page>
                        <fields>
                            <field name="createDateTime" label="创建时间" defaultValue="${CURRENT_DATETIME}"
                                   inputType="default"/>
                            <field name="status" label="创建时间" defaultValue="1" inputType="default"/>
                        </fields>
                    </page>
                </do>
                <do name="removeMaster" label="删除大师" access="" result="json"
                    execute="com.efeiyi.ec.system.master.controller.DeleteMasterRecommendedHandler"/>
                <do name="listMaster" label="传承人列表" access="" result="/master/masterList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="主键" name="id" defaultValue="0" operation="ne"/>
                    </query>
                </do>
                <do name="listMaster2" label="传承人列表" access=""
                    execute="com.efeiyi.ec.system.master.controller.TenantMasterListHandler"
                    result="/tenantMaster/masterList">
                    <query name="default" label="默认" type="auto" orderBy="id: desc">
                        <condition label="状态" name="status" defaultValue="1" operation="eq"/>
                    </query>
                </do>
                <do name="listMaster3" label="传承人列表" access=""
                    result="/tenantMaster/masterList">
                    <query name="default" label="默认" type="auto" orderBy="fullName: asc">
                        <condition label="状态" name="status" defaultValue="0" operation="ne"/>
                    </query>
                </do>
                <do name="plistBusiness" access="" label="传承人分页列表"
                    result="/master/masterPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                    </query>
                </do>

                <do name="formBusiness" label="保存大师页" access="" result="/master/masterForm"/>
                <do name="saveOrUpdateBusiness" label="保存大师" access="" result="/master/masterSaveSuccess">
                </do>
                <do name="removeBusiness" label="删除大师" access="" result="json"/>
                <do name="listBusiness" label="传承人列表" access="" result="/master/masterList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <!--<condition label="产品主键" name="id" defaultValue="1"o peration="eq"/>-->
                        <condition label="产品名称" name="name" defaultValue="1" operation="eq"/>
                        <!--<condition label="产品序列" name="serial" defaultValue="1" operation="eq"/>-->
                    </query>
                </do>

                <do name="listMasterTemp" label="传承人列表" access="" result="/master/masterList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="状态" name="status" defaultValue="0" operation="ne"/>
                        <condition label="大师名称" name="name" defaultValue="1" operation="eq"/>
                        <!--<condition label="产品序列" name="serial" defaultValue="1" operation="eq"/>-->
                    </query>
                </do>
            </does>
        </entity>

