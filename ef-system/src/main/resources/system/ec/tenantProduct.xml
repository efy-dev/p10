<?xml version="1.0" encoding="UTF-8"?>


<!--用户-->
<module name="tenantProduct" label="系统-传承人产品" version="" package="com.efeiyi.ec.tenant" path="">

    <entities>
        <entity name="TenantWorkList" label="传承人产品列表"
                basic="simple" others=""
                model="com.efeiyi.ec.tenant.model.TenantProduct">
            <fields>
                <field name="id" label="传承人产品id" inputType="text"/>
                <field name="name" label="传承人产品名字" inputType="text"/>
            </fields>
            <does>
                <do name="plistTenantProduct" access="" label="传承人产品分页列表"
                    result="/tenantProduct/tenantProductPList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="1" operation="eq"></condition>
                    </query>
                </do>

                <do name="formTenantProduct" label="保存传承人产品页" access="" result="/tenantProduct/tenantProductForm"></do>
                <do name="saveOrUpdateTenantProduct" label="保存传承人产品" access="" result="/tenantProduct/tenantProductSaveSuccess">
                </do>
                <do name="removeTenantProduct" label="删除大师产品" access="" result="json"/>
                <do name="listTenantProduct" label="大师著作产品" access="" result="/tenantProduct/tenantProductList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="产品id" name="id" defaultValue="1" operation="eq"/>
                    </query>
                </do>

            </does>
        </entity>

    </entities>

</module>