                        <condition label="产品主键" name="id" defaultValue="1" operation="eq"/>
                    </query>
                    <query name="default2" label="默认" type="auto" orderBy="sort:asc">
                        <condition label="product.id" name="product.id" defaultValue="1" operation="eq"/>
