        <entity name="ProductPicture" label="产品图片"
                basic="simple" others=""
                model="com.efeiyi.ec.product.model.ProductPicture">
            <does>
                <do name="plistProductPicture" access="" label="产品图片分页列表"
                    result="/productPicture/productPicturePList">
