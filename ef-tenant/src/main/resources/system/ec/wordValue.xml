<?xml version="1.0" encoding="UTF-8"?>


<!--用户-->
<module name="wordValue" label="wordValue" version="" package="com.ming800.core.p.model" path="">

    <entities>
        <entity name="WordValue" label="WordValue"
                basic="simple" others=""
                model="com.ming800.core.p.model.WordValue">
            <does>
                <do name="plistWordValue" access="" label="字典列表"
                    result="/wordValue/wordValuePList">
                    <query name="default" label="默认" type="auto" orderBy="group:asc">
                        <condition name="id" defaultValue="0" operation="ne"></condition>
                    </query>
                </do>
                <do name="viewWordValue" label="列表详情" result="/wordValue/wordValueView">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition name="id" defaultValue="0" operation="ne"/>
                    </query>
                </do>
                <do name="removeWordValue" label="删除" access="" result="json"/>
                <do name="formWordValue" label="编辑" access="" result="/wordValue/wordValueForm"></do>
                <do name="saveOrUpdateWordValue" label="保存" access="" result="/wordValue/wordValueSaveSuccess"></do>
                <do name="listWordValue" label="wordValue列表" access="" result="/wordValue/wordValueList">
                    <query name="default" label="默认" type="auto" orderBy="id:desc">
                        <condition label="主键" name="id" defaultValue="0" operation="ne"/>
                    </query>
                </do>

            </does>
        </entity>

    </entities>
</module>